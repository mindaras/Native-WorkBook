{
  "expo": {
    "name": "WorkBook",
    "slug": "workbook",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindaugaslazauskas.workbook"
    },
    "android": {
      "package": "com.mindaugaslazauskas.workbook"
    },
    "description": "WorkBook application for iOS created with React Native.",
    "githubUrl": "https://github.com/mindaras/Native-WorkBook"
  }
}
